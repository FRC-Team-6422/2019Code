{
  "java.configuration.updateBuildConfiguration": "disabled",
  "java.import.gradle.enabled": false,
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "bin/": true,
    ".classpath": true,
    ".project": true
  },
  "wpilib.online": true,
  "files.associations": {
    "*.inc": "cpp",
    "string": "cpp",
    "memory": "cpp",
    "iostream": "cpp",
    "atomic": "cpp",
    "thread": "cpp",
    "xstring": "cpp",
    "cmath": "cpp"
  },
  "python.linting.pylintEnabled": false,
  "python.formatting.provider": "black",
  "python.pythonPath": "C:\\Python30\\python.exe",
  "C_Cpp.configurationWarnings": "Disabled",
  "C_Cpp.default.configurationProvider": "vscode-wpilib"
}
